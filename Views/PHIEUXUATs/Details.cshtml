@model QLsach.Models.XUATModel

@{
    ViewBag.Title = "Details";
}

<h2>Chi tiết</h2>

<div>
    <h4>PHIEUXUAT</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Ngày")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.pHIEUXUAT.NGAY)
        </dd>

        <dt>
            @Html.DisplayName("Đại lý")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.pHIEUXUAT.DAILY.TENDL)
        </dd>

    </dl>
    <table class="table">
        <tr>
            <th>
                SÁCH
            </th>
            <th>
                Số lượng
            </th>
        </tr>

        @foreach (var item in Model.cTPXes)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SACH.TENSACH)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SL)
                </td>
            </tr>
        }
    </table>
</div>
<p>
    @Html.ActionLink("Về", "Index")
</p>
